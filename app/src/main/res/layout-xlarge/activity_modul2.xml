<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_modul2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mulyanto.dinacom.Modul2">

    <Button
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:layout_marginTop="62dp"
        android:id="@+id/btnhdd"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/hover_hdd" />

    <Button
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:layout_marginTop="15dp"
        android:id="@+id/btncmos"
        android:background="@drawable/hover_cmos"
        android:layout_below="@+id/btnhdd"
        android:layout_alignLeft="@+id/btnhdd"
        android:layout_alignStart="@+id/btnhdd" />

    <Button
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:id="@+id/btnletak"
        android:background="@drawable/hover_tata"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/btncmos"
        android:layout_alignStart="@+id/btncmos"
        android:layout_marginBottom="63dp" />

    <Button
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:id="@+id/btnheatsink"
        android:background="@drawable/hover_heatsink"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/btnbios"
        android:layout_alignLeft="@+id/btnletak"
        android:layout_alignStart="@+id/btnletak" />

    <Button
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:id="@+id/btnbios"
        android:background="@drawable/hover_bios"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/btnheatsink"
        android:layout_alignStart="@+id/btnheatsink" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:id="@+id/btnback"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/back" />
</RelativeLayout>
