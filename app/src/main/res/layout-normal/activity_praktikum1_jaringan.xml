        android:layout_height="110dp"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/btnsalahpr13"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/btnsalahpr12"
        android:layout_alignStart="@+id/btnsalahpr12"
        android:layout_marginBottom="60dp"
        android:background="@android:color/transparent" />

    <Button
        android:id="@+id/btnsalahpr12"
        android:layout_height="110dp"
        android:layout_width="200dp"
        android:layout_above="@+id/btnsalahpr13"
        android:layout_alignLeft="@+id/btnsalahpr11"
        android:layout_alignStart="@+id/btnsalahpr11"
        android:background="@android:color/transparent" />

</RelativeLayout>
